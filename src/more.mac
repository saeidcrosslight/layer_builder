$ --- energy parameters
eg0_well variation=function
function(xw,yw,xb,yb,temper)
e_GaN=3.507-9.09d-4*temper**2/(830+temper);
e_AlN=6.23-17.99d-4*temper**2/(1462+temper);
e_InN=0.735-2.45d-4*temper**2/(624+temper);
bow_AlGaN=-1;
bow_InGaN=-3;
bow_AlInN=-7;
et_AlGaN=(1-xw)*e_GaN+xw*e_AlN+(1-xw)*xw*bow_AlGaN; 
yt=(1-xw+yw)/2;
et_AlInN=(1-yt)*e_AlN+yt*e_InN+(1-yt)*yt*bow_AlInN; 
et_InGaN=yw*e_InN+(1-yw)*e_GaN+(1-yw)*yw*bow_InGaN;
eg300=((1-xw-yw)*xw*et_AlGaN+xw*yw*et_AlInN+(1-xw-yw)*yw*et_InGaN)
 /((1-xw-yw)*xw+xw*yw+(1-xw-yw)*yw);
eg300
end_function
$
eg0_bar variation=function
function(xw,yw,xb,yb,temper)
e_GaN=3.507-9.09d-4*temper**2/(830+temper);
e_AlN=6.23-17.99d-4*temper**2/(1462+temper);
e_InN=0.735-2.45d-4*temper**2/(624+temper);
bow_AlGaN=-1;
bow_InGaN=-3;
bow_AlInN=-7;
et_AlGaN=(1-xb)*e_GaN+xb*e_AlN+(1-xb)*xb*bow_AlGaN;
yt=(1-xb+yb)/2;
et_AlInN=(1-yt)*e_AlN+yt*e_InN+(1-yt)*yt*bow_AlInN;
et_InGaN=yb*e_InN+(1-yb)*e_GaN+(1-yb)*yb*bow_InGaN;
eg300=((1-xb-yb)*xb*et_AlGaN+xb*yb*et_AlInN+(1-xb-yb)*yb*et_InGaN)
 /((1-xb-yb)*xb+xb*yb+(1-xb-yb)*yb);
eg300
end_function
