material type=resistor 
resistivity  value=43.1d-8
affinity value=4.4
dielectric_constant value=-5.1953d2
$
real_index variation=function
function(wavelength)
xev=1.24/wavelength;
4.516562+1.694332*xev-5.341065*xev**2
  +2.811016*xev**3-0.401754*xev**4
end_function
absorption variation=function
function(wavelength)
xlam=wavelength;
xev=1.24/xlam;
betta=4.074419;
gamma=-0.405374;
kappa=betta*xev**gamma;
12.56d6*kappa/xlam
end_function
thermal_kappa value=22.
end_macro ti

