function(x,temper)
0.17+0.057*x-0.095*x**2+sqrt(4.e-4+2.56e-7*temper**2)
end_function

$ lowest bandgap is here
eg0_well variation=function 
function(x,temper)
egt=ext_func1;
egt
end_function

band_offset value=0.5

cond_band1_valley value=4

$ general formulas
$ 1/m=(costheta)**2/m_l + (sintheta)**2/m_t
$ theta = angle between 100 and 111
$ 1/m=(1/3)(2/m_t + 1/m_l)

cond1_mass_para value=0.05
cond1_mass_perp value=0.05
val1_mass_para value=0.05
val1_mass_perp value=0.05

$ e-dependent mass to account for non-parabolicty
$ unit=1/eV
$
$ e-dep. mass formula:
$ m(E)=m_parab+e_dep_mass1*E+e_dep_mass2*E^2
$ where m_parab = parabolic mass
$ where m_parab is parabolic, constant mass
$ Energy disperson:
$ E=hbar^2/(2*m0*m(E))*k^2

cond1_perp_e_dep_mass1 variation=function 
function(x)
egt=ext_func1;
em_parab=0.05;
2.*em_parab/egt
end_function

val1_perp_e_dep_mass1 variation=function 
function(x)
em_parab=0.05;
egt=ext_func1;
2.*em_parab/egt
end_function


cond1_para_e_dep_mass1 variation=function 
